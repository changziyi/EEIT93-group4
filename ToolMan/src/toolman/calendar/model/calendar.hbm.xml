<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="toolman.calendar.model.CalendarVO" table="toolmancalendar" >
        
        <id name="calen_id" column="calen_id">
            <generator class="identity"/>
        </id>
        

        <property name="event_id" column="event_id" />
        <property name="event_title" column="event_title" />
        <property name="event_start" column="event_start" />
        <property name="event_end" column="event_end" />
        <property name="event_className" column="event_className" />
        <property name="event_overlap" column="event_overlap" />
        <property name="event_allDay" column="event_allDay" />
		<property name="m_id" column="m_id" />


    </class>
</hibernate-mapping>

